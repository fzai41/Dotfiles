!Xterm
XTerm.vt100.faceName: Liberation Mono:size=10
!XTerm.vt100.faceName: Inconsolata:size=10

!URxvt
!URxvt.font: 				xft:m+1mn:regular:size=10
URxvt.font:				xft:Mplus Nerd Font Mono:pixelsize=14:antialiase=true
!URxvt.font:	                        xft:FantasqueSansMono Nerd 
Font:pixelsize=16:antialiase=true,\
					xft:Inconsolata Nerd Font Mono
!					xft:NanumGothic:regular:pixelsize=10:antialiase=true
URxvt.scrollBar: 			false
URxvt*geometry: 			80x15

!------------------------------------------------------------------------------
! ROFI Color theme
!------------------------------------------------------------------------------
                                                     
rofi.color-enabled: true
rofi.color-window: #1F1F1F, #1F1F1F, #1F1F1F
rofi.color-normal: #1F1F1F, #FFFFFF, #1F1F1F, #1F1F1F, #01B58A
rofi.color-active: #1F1F1F, #b48ead, #1F1F1F, #1F1F1F, #93e5cc
rofi.color-urgent: #1F1F1F, #ebcb8b, #1F1F1F, #1F1F1F, #ebcb8b


